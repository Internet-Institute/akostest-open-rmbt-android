<resources xmlns:tools="http://schemas.android.com/tools">
    <color name="colorPrimary">@android:color/white</color>
    <color name="colorPrimaryDark">@android:color/white</color>
    <color name="colorAccent">#59b200</color>

    <!-- Theme colors -->
    <color name="toolbar_blue">#00ACB1</color>
    <color name="toolbar_gray">#F2F3F5</color>

    <!-- Font colors -->
    <color name="black_40">#40000000</color>

    <color name="main_inactive_color">#1A000000</color>
    <color name="main_color">#0958BD</color>

    <color name="measurement_progress_empty">@color/main_inactive_color</color>
    <color name="measurement_progress_start">@color/main_color</color>
    <color name="measurement_progress_center" tools:ignore="UnusedResources">#00ACB1</color>
    <color name="measurement_progress_end" tools:ignore="UnusedResources">#00ACB1</color>

    <!-- Bottom navigation icon colors -->
    <color name="navigation_green">@color/colorAccent</color>
    <color name="navigation_gray">#5f6368</color>

    <!-- Ripple effect color  -->
    <color name="greenRipple">#4259b200</color>
    <color name="blackRipple">#26000000</color>

    <!--  White color with transparency -->
    <color name="text_white_transparency_40">#66FFFFFF</color>
    <!--  Gray color with transparency -->
    <color name="text_gray_transparency_40">#66424242</color>

    <!-- Text colors -->
    <color name="text_gray">#545454</color>
    <color name="text_dark_gray">#424242</color>

    <!-- Horizontal line divider color -->
    <color name="horizontal_divider">#eeeeee</color>
    <color name="horizontal_divider_white_10">#1AFFFFFF</color>

    <!-- Use in setting screen. Preference category title color -->
    <color name="text_gray_transparency_55">#8C5F6368</color>
    <!-- Use in setting screen, Preference item summary text color -->
    <color name="text_gray_transparency_60">#99424242</color>
    <!-- Use in IPInfoDialog, ip address value text color -->
    <color name="text_black_transparency_60">#99000000</color>
    <color name="text_black_transparency_20">#33000000</color>

    <color name="wave_primary">@android:color/white</color>
    <color name="wave_secondary">#33FFFFFF</color>

    <!-- Chart colors -->
    <color name="chart_labels_color">#8CFFFFFF</color>
