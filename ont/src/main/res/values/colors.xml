    <color name="measurement_bottom_background">#282828</color>
    <color name="measurement_scale">#3a3a3a</color>
    <color name="measurement_not_progressed">#7a7a7a</color>

    <color name="classification_green_dark">#007C0E</color>
    <color name="classification_green">#59B200</color>
    <color name="classification_yellow">#FFBA00</color>
    <color name="classification_red">#F5001C</color>
    <color name="classification_undefined">#F2F3F5</color>

    <color name="map_circle_fill">#56AD00</color>
    <color name="map_circle_stroke">#007C0E</color>

    <color name="ping_bar_color">#4D78Ed03</color>

    <color name="colorSelector">#5559b200</color>

    <color name="speed_graph_gradient_fill_start">#26004394</color>
    <color name="speed_graph_gradient_fill_end">#0Df2f6fa</color>
